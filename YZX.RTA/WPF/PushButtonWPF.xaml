<UserControl 
  x:Class="YZX.WINCC.Controls.PushButtonWPF"
  xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
  xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
  xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
  xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
  xmlns:pcp="clr-namespace:AITSW.PCPANEL.WPF;assembly=PCPANEL_WPF_Controls"
  mc:Ignorable="d" 
  d:DesignWidth="145"
  d:DesignHeight="145">
  
  <Grid Height="145" Width="145">
  <pcp:PushButton x:Name="PushButton">
    <pcp:PushButton.Colors>
      <pcp:ControlColors AuxiliaryColor1="Silver">
        <pcp:ControlColors.DefaultColoring>
          <pcp:ColoringColor Color1="Crimson" />
        </pcp:ControlColors.DefaultColoring>
      </pcp:ControlColors>
    </pcp:PushButton.Colors>
      <Label FontSize="20">开始</Label>
  </pcp:PushButton>
  </Grid>
</UserControl>
